{
  "name": "@mobius-network/web-wallet",
  "version": "0.0.1",
  "description": "Mobius Network Web wallet",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --hot --config webpack.development.config.js",
    "start:bleed": "webpack-dev-server --hot --config webpack.bleed.config.js",
    "clean": "rimraf dist",
    "build:beta": "cross-env npm run clean && webpack --config webpack.beta.config.js --progress --profile --colors",
    "build:production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "format": "prettier --write src/**/*.js *.js",
    "lint": "eslint --fix . --ext js",
    "lint-staged": "lint-staged",
    "create:component": "plop component",
    "create:state": "plop state",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "@babel/runtime": "7.0.0-beta.56",
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-regular": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.0.20",
    "@mobius-network/core": "0.0.1",
    "@mobius-network/mobius-client-js": "^0.3.2",
    "cross-env": "5.2.0",
    "downloadjs": "^1.4.7",
    "event-filter-redux-middleware": "^0.0.2",
    "grid-styled": "^4.1.0",
    "honeybadger-js": "^0.5.4",
    "i18next": "^11.3.3",
    "i18next-browser-languagedetector": "^2.2.0",
    "immutability-helper": "^2.7.0",
    "lodash": "^4.17.10",
    "nanoid": "^1.0.7",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-ga": "^2.5.3",
    "react-gtm-module": "^2.0.2",
    "react-hot-loader": "^4.1.2",
    "react-i18next": "^7.7.0",
    "react-modal": "^3.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "4.3.1",
    "react-toastify": "^4.1.0",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "redux-yo": "^0.0.26",
    "reselect": "^3.0.1",
    "revalidate": "^1.2.0",
    "stellar-hd-wallet": "^0.0.6",
    "stellar-sdk": "^0.8.1",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.56",
    "@babel/core": "7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.56",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.56",
    "@babel/plugin-transform-runtime": "7.0.0-beta.56",
    "@babel/preset-env": "7.0.0-beta.56",
    "@babel/preset-react": "7.0.0-beta.56",
    "@honeybadger-io/webpack": "^1.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "9.0.0-beta.3",
    "babel-jest": "23.4.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-react-hmre": "1.1.1",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "1.1.11",
    "git-revision-webpack-plugin": "^3.0.3",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "22.4.3",
    "lint-staged": "^7.1.0",
    "loader-utils": "^1.1.0",
    "open-browser-webpack-plugin": "0.0.5",
    "plop": "^2.0.0",
    "prettier": "^1.12.1",
    "react-test-renderer": "16.3.2",
    "rimraf": "2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "1.0.1",
    "webapp-webpack-plugin": "^2.2.0",
    "webpack": "4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "3.1.4"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
